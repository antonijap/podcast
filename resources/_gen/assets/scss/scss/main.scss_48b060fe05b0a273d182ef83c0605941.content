body{background:#f8fafd;font-family:source sans pro,sans-serif;font-size:16px}h1,h2,h3{font-family:roboto condensed,sans-serif;text-transform:uppercase;font-weight:700}.left-sidebar{background:#fff;padding:48px}.left-sidebar .static-content h1{font-size:24px}.left-sidebar .static-content p{line-height:33px;font-size:22px}.left-sidebar .static-content img{margin-bottom:32px}.episodes{width:80%;padding:48px 24px}.episodes .bold-small{font-weight:700;font-size:1em;text-transform:uppercase;color:#79869e;margin-bottom:16px}.episodes .episode{background:#fff;padding:24px;margin-bottom:16px;border-radius:4px}.episodes .episode h3{font-size:24px}